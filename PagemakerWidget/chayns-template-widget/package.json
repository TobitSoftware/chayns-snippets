{
  "name": "chayns_template_widget",
  "version": "1.0.0",
  "description": "Template for building chayns pagemaker widgets using ES2015",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --colors --progress --config ./webpack/dev.babel.js --host 0.0.0.0 --port 8080",
    "release": "webpack --colors --progress --config ./webpack/prod.babel.js",
    "setup": "mkdirp ./src/utils && mkdirp ./src/components/admin_view && mkdirp ./src/components/user_view"
  },
  "keywords": [
    "chayns",
    "pagemaker",
    "iframe",
    "widget",
    "chayns-templates",
    "es2015",
    "es6",
    "webpack",
    "tobit"
  ],
  "author": "Benedikt Schulze Baek",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.1",
    "html-webpack-plugin": "^2.26.0",
    "mkdirp": "^0.5.1",
    "sass-loader": "^6.0.1",
    "node-sass": "^4.5.0",
    "object-assign": "^4.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "webpack-replace": "^1.0.0"
  }
}
